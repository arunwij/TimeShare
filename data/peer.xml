<config>
	<max_peers>2</max_peers>
	<args>2</args>
	<method_name>add</method_name>
	<class_name>JCudaVectorAdd</class_name>
	<arg>
		<no>3</no>
		<type>d_array</type>
		<data_type>String</data_type>
		<data file='true'>filedata.csv</data>
		<split>
			<type>row</type>
			<min_count>2</min_count>
		</split>
	</arg>
	<arg>
		<no>4</no>
		<type>2d_array</type>
		<data_type>int</data_type>
		<data file='false'>[100,200,300,400],[500,600,700,800],[900,1000,1100,1200],[1300,1400,1500,1600]</data>
		<split>
			<type>row</type>
			<min_count>2</min_count>
		</split>
	</arg>
	<arg>
		<no>4</no>
		<type>1d_array</type>
		<data_type>int</data_type>
		<data file='false'>[1000,2000,3000,4000]</data>
		<broadcast>
			<type>row</type>
			<min_count>2</min_count>
		</broadcast>
	</arg>
